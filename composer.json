{
    "name": "mralston/laravel-additional-string-helpers",
    "description": "Adds additional helpers to Laravel's Str and Stringable helpers.",
    "keywords": ["laravel", "string", "helpers"],
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/mralston/laravel-additional-string-helpers/issues",
        "source": "https://github.com/mralston/laravel-additional-string-helpers"
    },
    "authors": [
        {
            "name": "Matt Ralston",
            "email": "matt@mralston.co.uk"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MRalston\\LaravelAdditionalStringHelpers\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MRalston\\LaravelAdditionalStringHelpers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
